// Place your key bindings in this file to override the defaultsauto[]
[
   // Toggle between terminal and editor focus
{"key": "ctrl+9", 
  "command": "workbench.action.terminal.focus"}, // focusTerminal
{"key": "ctrl+9", 
 "command": "workbench.action.focusActiveEditorGroup", //unFocusTerminal
 "when": "terminalFocus"},
{"key": "ctrl+tab", 
 "command": "workbench.action.nextEditor"}, //navigate between tabs
{"key": "ctrl+shift+tab",
 "command": "workbench.action.previousEditor"}, //navigate between tabs
{"key": "ctrl+w",
 "command": "workbench.action.closeActiveEditor"}, // close current window 

{"key": "ctrl+alt+f",
 "command": "workbench.action.quickOpen"}, //fast search in files 
{"key": "ctrl+p",
 "command": "-workbench.action.quickOpen"}, // CLEAN

{"key": "ctrl+l",
 "command": "workbench.action.quickOpenNavigatePreviousInFilePicker", // open navBar 
 "when": "inFilesPicker && inQuickOpen"
},
{
   "key": "ctrl+shift+e",
   "command": "-workbench.action.quickOpenNavigatePreviousInFilePicker", //CLEAN
   "when": "inFilesPicker && inQuickOpen"
},
{
   "key": "ctrl+l",
   "command": "workbench.view.explorer" // viev navBar 
},
{
   "key": "ctrl+shift+e",
   "command": "-workbench.view.explorer" //CLEAN
},
{
   "key": "ctrl+l",
   "command": "workbench.action.toggleSidebarVisibility", //toggle navBar
   "when": "sideBarFocus"
},
{
   "key": "ctrl+b",
   "command": "-workbench.action.toggleSidebarVisibility" //CLEAN
},
{
   "key": "ctrl+7",
   "command": "workbench.action.terminal.focusNext",
   "when": "terminalFocus"
},
{
   "key": "ctrl+7",
   "command": "-workbench.action.focusSeventhEditorGroup"
},
{
   "key": "ctrl+8",
   "command": "workbench.action.terminal.focusNextPane",
   "when": "terminalFocus"
},
{
   "key": "ctrl+6",
   "command": "-workbench.action.focusSixthEditorGroup"
},
{
   "key": "ctrl+6",
   "command": "workbench.action.terminal.split",
   "when": "terminalFocus && terminalProcessSupported"
},
{
   "key": "ctrl+shift+5",
   "command": "-workbench.action.terminal.split",
   "when": "terminalFocus && terminalProcessSupported"
}
]